{
	"name": "matthias.harzer.dev",
	"version": "1.0.0",
	"devDependencies": {
		"@biomejs/biome": "2.2.0",
		"@lit/localize": "0.12.2",
		"@lit/localize-tools": "0.8.0",
		"@lit/task": "1.0.3",
		"esbuild": "0.25.9",
		"lit": "3.2.1",
		"typescript": "5.9.2"
	},
	"scripts": {
		"analyze": "npx biome ci ./src",
		"fix": "npx biome check --write ./src index.html index.css",
		"build": "npx esbuild --bundle --outdir=dist --minify --keep-names src/index.ts",
		"typecheck": "npx tsc",
		"test": "echo not implemented yet",
		"watch": "npx esbuild src/index.ts --bundle --outdir=dist --watch --sourcemap --keep-names"
	}
}
